               ТМП2 = (ТМП2 as Вариант[2]);
               Для Y = 1.. Данные.RowsCount цикл
                 для X = 1.. Данные.ColumnsCount цикл
                    Contents = Данные.Клетка[X,Y].Contents ;
                    если НеПриводить Тогда --Нет Типа полей!
                      ВставитьВМассив(ТМП2[Y], X, Contents);
                    Иначе

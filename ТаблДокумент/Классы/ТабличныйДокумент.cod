  proc Уничтожение Синоним Destroy;

    var p, p1: Вариант ;

    p = Current;
    p1 = p.Хозяин;
    while p1 <> nil do
      p = p1;
      p1 = p1.Хозяин;
      p.Хозяин = nil;
    od;
    Current.Хозяин = nil;
    Если Current.БланкМакета = nil Тогда
    Иначе
      Current.БланкМакета.Destroy ;
      Current.БланкМакета   = nil;
    Илсе;

    Current.БланкПриемник = nil;


    p = Self;
    p1 = p.Хозяин;
    while p1 <> nil do
      p = p1;
      p1 = p1.Хозяин;
      p.Хозяин = nil;
    od;
    Self.Хозяин        = nil;
    Если Self.БланкМакета = nil Тогда
    Иначе
      Self.БланкМакета.Destroy ;
      Self.БланкМакета   = nil;
    Илсе;
    Self.БланкПриемник = nil;

  end;

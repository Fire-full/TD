            end;
            Если Типы.RowsCount    = 1 Тогда -- Только Так!!
              Горизонтально = Истина;
              для Y = 1.. Типы.ColumnsCount цикл
                ВставитьВМассив(ТипыПолей, Y, ВернутьТипПоля(Типы.Клетка[Y,1].Contents) );
